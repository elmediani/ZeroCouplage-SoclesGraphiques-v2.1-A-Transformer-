<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
	<display-name>ZCAppsDemo</display-name>
	<display-name>zerocouplage-test</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	<filter>
		<filter-name>zeroCouplageWebManager</filter-name>
		<filter-class>com.zerocouplage.web.manager.ZCComponentVirtualWeb</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>zeroCouplageWebManager</filter-name>
		<url-pattern>*.zc</url-pattern>
	</filter-mapping>
</web-app>